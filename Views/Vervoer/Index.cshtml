@{
    var cc = TempData["numbers"] as int[,];
    int first = 1950;
}

@model List<Infrastructuur.Entities.Enquete>;

<table class="table">
  <thead>
    <tr>
      <th scope="col">Year</th>
      <th scope="col">E</th>
      <th scope="col">F</th>
      <th scope="col">OV</th>
      <th scope="col">A</th>
    </tr>
  </thead>
  <tbody>
  
 @for (int i = 0; i < cc.GetLength(0);i++)
{
    <tr>
        <td>@(first += 10)</td>
    @for (int j = 0; j < cc.GetLength(1); j++)
    {
          <td>@cc[i,j]</td>
    }
    </tr>
}
  </tbody>
</table>